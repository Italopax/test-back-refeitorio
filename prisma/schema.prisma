// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int    @id @default(autoincrement())
  email String @unique
  name  String
  @@map("users") // nome da tabela 
}

model Menu {
  id Int @id @default(autoincrement())
  snack String
  date DateTime
  items String
  @@map("menu ")
}
